<%= form_tag({:action => 'create_negative'}, {:class => 'form-container'}) do %>
  <%= raw negative_captcha(@captcha) %>
  <%= negative_text_field_tag(@captcha, :username, :class => "cool-input") %>
  <%= negative_text_field_tag(@captcha, :password, :class => "cool-input") %>
  <%= submit_tag "Submit Negative", :class => "cool-button" %>
<% end %>

